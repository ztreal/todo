<?xml version="1.0" encoding="GBK" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">


<struts>

    <package name="jsonOrder" namespace="/json/order" extends="json-protected-default">

       <action name="order_*" method="{1}" class="com.easytodo.com.web.action.order.SerchYuwangAction">
           <result name="searchYunState" type="json">
                <param name="ignoreHierarchy">true</param>
                <param name="excludeNullProperties">true</param>
                <param name="root">saleStateCnList</param>
            </result>
        </action>

        <action name="log_*" method="{1}" class="com.easytodo.com.web.action.order.LogAction">
           <result name="logList" type="json">
                <param name="ignoreHierarchy">true</param>
                <param name="excludeNullProperties">true</param>
                <param name="root">logList</param>
            </result>
        </action>

    </package>


</struts>
